#pragma once
#include "component.h"
#include "dispacher_t.h"

<% dispatchers.each do |e| %>
<%# The signature type of callbacks %>
typedef void
<%= e.name_sanitized %>_callback_f(struct common_data_s *common_data,
                                   <%= e.args.map{ |s| s.type }.join(", ") %>);
<%# The Function who register the callbacks to the dispatcher %>
void btx_register_callbacks_<%= e.name_sanitized %>(
    <%= hash_type %> **<%= hash_name %>,
    <%= e.name_sanitized %>_callback_f *callback);
<% end %>
